{
  ".switch": {
    "width": 200,
    "touchEnabled": true,
    "toggle": false,
    "thumbOnColor": "#00e600",
    "thumbOffColor": "#CCCCCC",
    "toggleOffColor": "#010101",
    "toggleOnColor": "#CCCCCC",
    "visible": true,
    "alpha": 1,
    "borderColor": "rgba(0,0,0,1)",
    "borderWidth": 0,
    "font": {},
    "flexProps": {
      "alignSelf": "AUTO",
      "positionType": "RELATIVE"
    },
    "minHeight": 31,
    "minWidth": 51,
    "&-small": {
      "width": 100
    },
    "&-medium": {
      "width": 250
    },
    "&-large": {
      "width": 350
    },
    "&-warning": {
      "backgroundColor": "#9F6000"
    },
    "&-error": {
      "backgroundColor": "#D8000C"
    },
    "&-success": {
      "backgroundColor": "#4F8A10"
    }
  }
}
